n=int(input("Enter The N Value : "))
n1,n2 = 0, 1
count = 0
if n<=0:
   print("Please Enter A Positive Integer.")
elif n == 1:
   print("Fibonacci sequence upto",n,":",str(n1))
else:
   print("Fibonacci sequence:")
   while(count<n):
       print(n1,end=" ")
       n3=n1 + n2
       n1,n2=n2,n3
       count += 1
